<?xml version="1.0" encoding="UTF-8"?>
<image-types>
	
    <image-type>
        <name>VMware Virtual Machine Image</name>
        <description>VMware Virtual Machine Image</description>
        
        <property-definitions>
        	<property-definition>
				<name>Image Path</name>
				<description>Path to image under VMs and Templates inventory view</description>
				<required>true</required>
				<type>STRING</type>
				<default-string-value></default-string-value>
				<string-max-length>200</string-max-length>
				<allowed-string-values></allowed-string-values>
				<default-file-value></default-file-value>
			</property-definition>        
        </property-definitions>
        
        <supported-compute-center-types>
            <compute-center-type>VMware vCenter Server</compute-center-type>
        </supported-compute-center-types>
    </image-type>	
	
</image-types>